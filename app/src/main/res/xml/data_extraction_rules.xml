<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="important_data/" />
        <include domain="database" path="user_data.db" />
        <include domain="sharedpref" path="user_preferences.xml" />
        <exclude domain="file" path="cache/" />
        <exclude domain="database" path="temp_data.db" />
        <exclude domain="sharedpref" path="temp_preferences.xml" />
        <!-- TODO: Use <include> and <exclude> to control what is backed up.
        <include .../>
        <exclude .../>
        -->
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>